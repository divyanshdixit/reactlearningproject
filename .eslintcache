(\r\n        <>\r\n            <p> Current time : {currentTime} </p>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default ClockDigital;","D:\\reactlearningproject\\src\\Components\\ClassCompoWithConstructor.jsx",[],"D:\\reactlearningproject\\src\\Components\\ClassCompoA.jsx",[],"D:\\reactlearningproject\\src\\Components\\BindEventInClass.jsx",[],"D:\\reactlearningproject\\src\\Components\\Counters.jsx",[],"D:\\reactlearningproject\\src\\Components\\FormsReact3.jsx",[],"D:\\reactlearningproject\\src\\Components\\GetPropAsFun.jsx",[],"D:\\reactlearningproject\\src\\Components\\ReactForms.jsx",[],"D:\\reactlearningproject\\src\\Components\\PureComponent.jsx",["232"],"import React, { Component, PureComponent } from 'react'\r\n\r\n// shallow comparison \r\n// if value is changed the inly it'll re-render the component\r\n\r\n// pureCompo => implements shouldComponentUpdate() with shallow comparison\r\n// regular comp => doesn't implement shouldComponentUpdate()\r\n\r\nclass ComponentPure extends PureComponent {\r\n    constructor(props) {\r\n        super(props)\r\n    \r\n        this.state = {\r\n             name: 'Divyansh',\r\n             city:'asdas'\r\n        }\r\n    }\r\n\r\n    componentDidMount(){\r\n        setInterval( () => {this.setState({\r\n            name: 'Abhushek'\r\n        })}, 3000)\r\n    }\r\n    \r\n    // Shallow comparison\r\n    // ==, ===\r\n    // '1' === 1 return false\r\n\r\n    // a == a\r\n    /*\r\n    var a = [1,2,3], {}\r\n    var b = a\r\n    var c = b;\r\n    reference\r\n    // call by value\r\n    // call by reference\r\n\r\n    a === b false [value = = = reference]]\r\n    b === c true\r\n    */\r\n\r\n\r\n    render() {\r\n        console.log('object')\r\n        console.log('Pure compo render method')\r\n        return (\r\n            <div>\r\n                Pure Component {this.state.name}\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default ComponentPure","D:\\reactlearningproject\\src\\Components\\RefInClassParent.jsx",[],"D:\\reactlearningproject\\src\\Components\\RefsComponent.jsx",[],"D:\\reactlearningproject\\src\\Components\\MemoComponent.jsx",[],"D:\\reactlearningproject\\src\\Components\\PassFunAsProps.jsx",[],"D:\\reactlearningproject\\src\\Components\\ToDoApp.jsx",[],"D:\\reactlearningproject\\src\\Components\\RefForwardingParent.jsx",[],"D:\\reactlearningproject\\src\\Components\\HocHoverCounter.jsx",[],"D:\\reactlearningproject\\src\\Components\\ContextComponentD.jsx",[],"D:\\reactlearningproject\\src\\Components\\LifeCycleA.jsx",["233"],"// import { ThreeSixtySharp } from '@material-ui/icons'\r\nimport React, { Component, PureComponent } from 'react'\r\nimport LifeCycleB from './LifeCycleB'\r\n\r\n// parent compo\r\nclass LifeCycleA extends Component {\r\n\r\n    // first method  \r\n    constructor(props) {\r\n        super(props);\r\n        \r\n        // state initialise\r\n        this.state = {\r\n             name: 'Divyansh'\r\n        }\r\n        console.log('parent const method')\r\n    }\r\n\r\n    clickHandler = () => {\r\n        this.setState({\r\n            name : 'Abhishek'\r\n        })\r\n    } \r\n\r\n    // second method , 1st method only for updatign phase\r\n    static getDerivedStateFromProps(props, state){\r\n       console.log(props, state);\r\n       console.log('parent Derived state method Lifecycle A');\r\n       return null;\r\n    }\r\n\r\n    // // 2nd method for updating\r\n    shouldComponentUpdate(updateProps, updateState){\r\n        console.log(updateProps, updateState);\r\n        if(this.state.name === updateState.name){\r\n            return false;\r\n        }\r\n        return true;\r\n    }\r\n    \r\n    // fourth method\r\n\r\n    componentDidMount(){\r\n        console.log('parent Compoent did mount method lifecycle A');\r\n    }\r\n\r\n    // // foruth\r\n    // getSnapshotBeforeUpdate(prevProps, prevState){\r\n    //     console.log('Get snap shot method for LC A ')\r\n    //     return null;\r\n    // }\r\n\r\n    // // fifth\r\n    // componentDidUpdate(prevProps, prevState, value){\r\n    //     console.log(value)\r\n    //     console.log('component updated');\r\n    // }\r\n\r\n    // third method\r\n    render() { // jsx \r\n        console.log('parent render method Lifecycle A')\r\n        return (\r\n            <div>\r\n                LifeCycle A\r\n\r\n                <button onClick={this.clickHandler}> Change State </button>\r\n                 {/*child component */}\r\n                <LifeCycleB name={this.state.name}/>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default LifeCycleA\r\n","D:\\reactlearningproject\\src\\Components\\TodoAppDemo.jsx",[],"D:\\reactlearningproject\\src\\Components\\About.jsx",[],"D:\\reactlearningproject\\src\\Components\\Books.jsx",[],"D:\\reactlearningproject\\src\\Components\\Home.jsx",[],"D:\\reactlearningproject\\src\\Components\\Contact.jsx",[],"D:\\reactlearningproject\\src\\Components\\HocClickCounter.jsx",[],"D:\\reactlearningproject\\src\\Components\\Error404.jsx",[],"D:\\reactlearningproject\\src\\Components\\RefInClassChild.jsx",[],"D:\\reactlearningproject\\src\\Components\\RefForwardingChild.jsx",[],"D:\\reactlearningproject\\src\\Components\\HocPattern.jsx",[],"D:\\reactlearningproject\\src\\Components\\ItemList.jsx",[],"D:\\reactlearningproject\\src\\Components\\ContextComponentE.jsx",["234"],"import React, { Component, useContext } from 'react'\r\nimport ContextComponentF from './ContextComponentF'\r\nimport {userContext, compoContext} from '../App';\r\n\r\nconst ContextComponentE = (props) => {\r\n\r\n    const fnameContext = useContext(userContext) // provide ()\r\n    const useCompoContext = useContext(compoContext) // provide ()\r\n\r\n    return (\r\n        <div>\r\n            Component E =  {fnameContext} == {useCompoContext}\r\n            <ContextComponentF/>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default ContextComponentE\r\n","D:\\reactlearningproject\\src\\Components\\Para.jsx",[],"D:\\reactlearningproject\\src\\Components\\LifeCycleB.jsx",[],"D:\\reactlearningproject\\src\\Components\\ToDoListItemsDemo.jsx",[],"D:\\reactlearningproject\\src\\Components\\ContextComponentF.jsx",["235"],"import React, { Component } from 'react'\r\nimport {Consumer, compoContext, userContext} from '../App';\r\n\r\nclass ContextComponentF extends Component {\r\n    render() {\r\n        return (\r\n                <userContext.Consumer>\r\n                    {\r\n                        (username) => {\r\n                            return (\r\n                                <compoContext.Consumer> \r\n                                {\r\n                                    (otherContext) => {\r\n                                        return <h1> Component F Hello {username} {otherContext}</h1>\r\n                                    }\r\n                                }\r\n                                \r\n                                \r\n                                </compoContext.Consumer>\r\n                            )\r\n                        }\r\n                    }\r\n                </userContext.Consumer>\r\n        )\r\n    }\r\n}\r\n\r\nexport default ContextComponentF\r\n","D:\\reactlearningproject\\src\\Components\\HookUseEffect.jsx",[],"D:\\reactlearningproject\\src\\Components\\UseReducerFetchData.jsx",["236"],"import React, {useEffect, useReducer} from 'react'\r\nimport axios from 'axios';\r\n\r\n// fetching data from api\r\n// show fetched data\r\n\r\n// reducer\r\n\r\n// case : FETCH_REQUEST\r\n// FETCH_SUCCESS\r\n// FETCH_FAIL\r\n\r\nconst initState = {\r\n    loading:false,\r\n    data:[],\r\n    error:''\r\n}\r\n\r\n// reducer is function =>? return new update state\r\n\r\n\r\nconst UseReducerFetchData = () => {\r\n\r\n    const postReducer = (state=initState, action) => {\r\n        switch(action.type){\r\n            case 'FETCH_REQUEST':\r\n                return {\r\n                    ...state,\r\n                    loading:true\r\n                }\r\n            case 'FETCH_SUCCESS':\r\n                console.log(state);\r\n                return {\r\n                    ...state,\r\n                    loading:false,\r\n                    data:action.payload\r\n                }\r\n            case 'FETCH_FAIL':\r\n                return {\r\n                    ...state,\r\n                    loading:false,\r\n                    error:action.payload\r\n                }\r\n\r\n        }\r\n    }\r\n    const [state, dispatch] = useReducer(postReducer, initState)\r\n\r\n    useEffect(() => {\r\n        dispatch({type:'FETCH_REQUEST'})\r\n        axios.get('https://jsonplaceholder.typicode.com/posts')\r\n            .then((res) => {\r\n                console.log(res.data);\r\n                const posts = res.data;\r\n                dispatch({type:'FETCH_SUCCESS', payload:posts})\r\n            })\r\n            .catch((err) => {\r\n                console.log(err)\r\n                const errMsg = err.message;\r\n                dispatch({type:'FETCH_FAIL', payload:errMsg})\r\n            })\r\n    }, [])\r\n\r\n    return (\r\n        <div>\r\n            {\r\n                state.loading ? (<h2> Loading... </h2>) : state.error ? (<h2> {state.error}</h2>) : (state.data.map((post) => (<p>{post.title} </p>))) \r\n            }\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default UseReducerFetchData\r\n","D:\\reactlearningproject\\src\\Components\\Services.jsx",[],"D:\\reactlearningproject\\src\\Components\\UseReducerHook.jsx",[],"D:\\reactlearningproject\\src\\Components\\NavBar.jsx",["237"],"import React from 'react'\r\nimport {Link, NavLink} from 'react-router-dom';\r\n\r\nconst NavBar = () => {\r\n    return (\r\n        <div>\r\n            {/* we'll not use <a> tag bcz it reload page so we use <Link> tag of react router dom it'll not reload page */}\r\n            {/* <Link to='/' > Link Home </Link>  Client side routing (page will not reload) */}\r\n            {/* <a href=\"/\"> A href home </a>  */} {/* server side routing (page will reload)*/}\r\n            {/* <Link to=\"/about\"> About </Link>\r\n            <Link to=\"/services\"> Services </Link>\r\n            <Link to=\"/contact\"> Contact </Link> */}\r\n\r\n        <br/>\r\n            {/* we can also use <NavLink> tag so it looks more cool as we can add exta attributes in it */}\r\n\r\n            <NavLink exact to='/' activeClassName=\"active\"> Home </NavLink>\r\n            <NavLink exact to='/about' activeClassName=\"active\"> About </NavLink>\r\n            <NavLink exact to='/about-us' activeClassName=\"active\"> AboutUs </NavLink>\r\n            <NavLink exact to='/services' activeClassName=\"active\"> Services </NavLink>\r\n            <NavLink exact to='/contact' activeClassName=\"active\"> Contact </NavLink>\r\n            <NavLink exact to='/users' activeClassName=\"active\"> Users </NavLink>\r\n            <NavLink exact to='/search' activeClassName=\"active\"> Search </NavLink>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default NavBar;","D:\\reactlearningproject\\src\\Components\\UseReducerHookEx2.jsx",["238"],"import React, {useReducer} from 'react'\r\n\r\nconst UseReducerHookEx2 = () => {\r\n\r\n    const init = {\r\n         counter: 0,\r\n         counter1:0\r\n    }\r\n\r\n    const reducer = (state, action) => {\r\n        switch(action.type){\r\n            case 'increment':\r\n                return {\r\n                    ...state, \r\n                    counter: state.counter + action.payload,\r\n                };\r\n            case 'increment2':\r\n                return {\r\n                    ...state,\r\n                    counter1: state.counter1 + action.payload\r\n                }\r\n        }\r\n    }\r\n\r\n    const [count, dispatch] = useReducer(reducer , init);\r\n\r\n    \r\n    return (\r\n        <div>\r\n            <h1> Count - {count.counter} {count.counter1}</h1>\r\n            <button onClick={() 